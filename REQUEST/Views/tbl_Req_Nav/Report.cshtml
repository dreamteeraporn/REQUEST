@model IEnumerable<REQUEST.Models.View_Req_NAV_Trace>

@{
    Layout = null;
}

<h2 align="lg">แบบฟอร์มการขอและเเก้ไขสิทธิในระบบ NAV</h2>
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />

<table class="table table-bordered table-hover text-nowrap fixed">
   
    <tr bgcolor="ffd800">

        <th>เลขเอกสาร</th>
        <th>วันที่แจ้ง</th>
        <th>รหัสพนักงาน</th>
        <th>ชื่อ-สกุล</th>
        <th>ตำแหน่ง</th>
        <th>ฝ่าย</th>

        <th>สิทธิการใช้งาน</th>
        <th>สถานะสิทธิ</th>
        <th>เลือกฝ่าย/ส่วน</th>
        <th>รายละเอียดและสิ่งที่ต้องการ</th>
        <th>เอกสารแนบ(ถ้ามี)</th>


        <th>ชื่อผู้อนุมัติ</th>
        <th>วันที่อนุมัติ</th>

        <th>ผู้เปิดงาน</th>
        <th>ชื่อผู้เปิดงาน</th>
        <th>วันเปิดงาน</th>

        <th>ผู้ดำเนินงาน</th>
