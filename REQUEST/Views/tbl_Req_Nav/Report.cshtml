        <th>วันดำเนินงาน</th>

        <th>ผู้ปิดงาน</th>
        <th>ชื่อผู้ปิดงาน</th>
        <th>วันปิดงาน</th>


    </tr>
    


    @foreach (var item in Model)
    {
<tr>
    <td>
        <center>@Html.DisplayFor(modelItem => item.Req_NAV_CODE)</center>
</td>
    <td>
        <center>@Html.DisplayFor(modelItem => item.DATE_NAV)</center>
</td>
    <td>
        <center> @Html.DisplayFor(modelItem => item.USER_NO)</center>
</td>

    <td>
        <center>  @Html.DisplayFor(modelItem => item.USER_NAME)</center>
</td>


    <td>
        <center> @Html.DisplayFor(modelItem => item.PROSITION)</center>
</td>

    <td>
        <center>@Html.DisplayFor(modelItem => item.DEPARTMENT)</center>
</td>


    <td>
        <center>  @Html.DisplayFor(modelItem => item.Allow_DESCRIPTION)</center>
</td>
    <td>
        <center> @Html.DisplayFor(modelItem => item.STA_DESCRIPTION)</center>
</td>

    <td>
        <center>  @Html.DisplayFor(modelItem => item.SECTION_DESC)</center>
</td>

    <td>
        <center>@Html.DisplayFor(modelItem => item.NAV_Detail)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.NAV_DOC)</center>
</td>


    <td>
        <center>  @Html.DisplayFor(modelItem => item.NAME_APPROVER)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.Date_APPROVER)</center>
</td>

    <td>
        <center>@Html.DisplayFor(modelItem => item.Req_OPEN_BY)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.NAME_OPEN)</center>
</td>

    <td>
        <center>@Html.DisplayFor(modelItem => item.Req_OPEN_DATE)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.Req_ASSIGN_TO)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.NAME_ASSIGN)</center>
</td>

    <td>
        <center> @Html.DisplayFor(modelItem => item.Req_ASSIGN_TO_DATE)</center>
</td>


    <td>
        <center> @Html.DisplayFor(modelItem => item.Req_CLOSE)</center>
</td>
    <td>
        <center>@Html.DisplayFor(modelItem => item.NAME_CLOSE)</center>
</td>


    <td>
        <center>@Html.DisplayFor(modelItem => item.Req_CLOSE_DATE)</center>
</td>


